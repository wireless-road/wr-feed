Subject: [PATCH 4/5] WR: Add NetSOM platform defconfig

---
 configs/mx6ull_wr_netsom_defconfig | 55 ++++++++++++++++++++++++++++++
 1 file changed, 55 insertions(+)
 create mode 100644 configs/mx6ull_wr_netsom_defconfig

diff --git a/configs/mx6ull_wr_netsom_defconfig b/configs/mx6ull_wr_netsom_defconfig
new file mode 100644
index 0000000000..bc1eaf8ac1
--- /dev/null
+++ b/configs/mx6ull_wr_netsom_defconfig
@@ -0,0 +1,55 @@
+CONFIG_ARM=y
+CONFIG_ARCH_MX6=y
+CONFIG_SYS_TEXT_BASE=0x87800000
+CONFIG_IMX_CONFIG="board/wirelessroad/mx6ull_wr_netsom/imximage.cfg"
+CONFIG_MX6ULL=y
+CONFIG_TARGET_MX6ULL_WR_NETSOM=y
+CONFIG_FIT=y
+CONFIG_DEFAULT_DEVICE_TREE="imx6ull-wr-netsom"
+CONFIG_SYS_PROMPT="WR2022=> "
+# CONFIG_BOOTM_NETBSD is not set
+# CONFIG_BOOTM_PLAN9 is not set
+# CONFIG_BOOTM_RTEMS is not set
+# CONFIG_BOOTM_VXWORKS is not set
+# CONFIG_CMD_BOOTEFI is not set
+CONFIG_CMD_DM=y
+CONFIG_CMD_GPIO=y
+CONFIG_CMD_MMC=y
+CONFIG_CMD_PART=y
+CONFIG_CMD_DHCP=y
+# CONFIG_BOOTP_HOSTNAME is not set
+# CONFIG_CMD_NFS is not set
+CONFIG_CMD_MII=y
+CONFIG_CMD_PING=y
+CONFIG_OF_CONTROL=y
+CONFIG_ENV_OVERWRITE=y
+# CONFIG_ENV_IS_NOWHERE is not set
+CONFIG_NET_RANDOM_ETHADDR=y
+CONFIG_GPIO_HOG=y
+# CONFIG_I2C is not set
+CONFIG_FSL_USDHC=y
+CONFIG_SF_DEFAULT_BUS=3
+CONFIG_SF_DEFAULT_SPEED=40000000
+CONFIG_SPI_FLASH_BAR=y
+CONFIG_SPI_FLASH_GIGADEVICE=y
+CONFIG_SPI_FLASH_MACRONIX=y
+CONFIG_SPI_FLASH_WINBOND=y
+CONFIG_SPI_FLASH_USE_4K_SECTORS=y
+CONFIG_PHYLIB=y
+CONFIG_PHY_MICREL=y
+CONFIG_PHY_MICREL_KSZ8XXX=y
+CONFIG_PHY_SMSC=y
+CONFIG_DM_MDIO=y
+CONFIG_FEC_MXC_SHARE_MDIO=y
+CONFIG_FEC_MXC_MDIO_BASE=0x02188000
+CONFIG_FEC_MXC=y
+CONFIG_MII=y
+CONFIG_PINCTRL=y
+CONFIG_PINCTRL_IMX6=y
+CONFIG_DM_REGULATOR=y
+CONFIG_DM_REGULATOR_ANATOP=y
+CONFIG_MXC_UART=y
+CONFIG_SPI=y
+CONFIG_MXC_SPI=y
+CONFIG_IMX_THERMAL=y
